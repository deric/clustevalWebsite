<% content_for :title, "Program Configuration '" + @programConfig.name + "'" %>
<table id="programConfig" class="display">
  <thead>
	<tr>
	<th>Information</th>
	<th>Value</th>
	</tr>
  </thead>
  <tbody>
  <tr>
    <td>Filename</td><td><%= @programConfig.absPath.split('/')[-1] %></td>
  </tr>
  <tr>
    <td>Program</td><td><%= link_to @program.name, @program %></td>
  </tr>
  <tr>
    <td>Runresult format</td><td><%= @runResultFormat.name %></td>
  </tr>
  <tr>
    <td>Parameters</td><td><% @parameters.each do |parameter| %><%= link_to parameter.name, parameter %><br /><% end %></td>
  </tr>
  <tr>
    <td>File content</td><td>
<pre>
<%= @contents %>
</pre></td>
  </tr>
  </tbody>
</table>

<table id="programConfig2" class="display">
  <thead>
  <tr>
    <th>Date</th>
    <th>Data Config</th>
    <th>Parameter set</th>
    <th>Clustering Quality Measure</th>
    <th>Quality</th>
    <th>Clustering</th>
  </tr>
  </thead>
  <tbody>
  </tbody>
</table>
<script type="text/javascript">
$(document).ready(function() {
  $('#programConfig2').dataTable({
      sPaginationType: "full_numbers",
      sDom: '<"H"Rlfr>t<"F"ip>',
      bJQueryUI: true,
      bRetrieve: true,
      bDestroy: true,
      aaSorting: [[ 4, "desc" ]],
      bProcessing: true,
      bDeferRender: true,
      sAjaxSource: '<%= '/program_configs/fetch_table_data/' + params[:id] %>'
    });
});
</script>