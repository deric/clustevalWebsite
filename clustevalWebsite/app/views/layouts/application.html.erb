  <script type="text/javascript">
    $(function(){
      var stateMachineConnector = {       
        connector:"StateMachine",
        paintStyle:{lineWidth:3,strokeStyle:"#056"},
        hoverPaintStyle:{strokeStyle:"#dbe300"},
        endpoint:"Blank",
        anchor:"Continuous",
        overlays:[ ["PlainArrow", {location:1, width:15, length:12} ]]
      };
      
      jsPlumb.connect({
          source:"usecase_1",
          target:"usecase_2"
      }, stateMachineConnector);
      jsPlumb.connect({
          source:"usecase_2",
          target:"usecase_3"
      }, stateMachineConnector);
      jsPlumb.connect({
          source:"usecase_3",
          target:"usecase_4"
      }, stateMachineConnector);
      jsPlumb.connect({
          source:"usecase_4",
          target:"usecase_5"
      }, stateMachineConnector);
      jsPlumb.connect({
          source:"usecase_5",
          target:"usecase_6"
      }, stateMachineConnector);
      jsPlumb.connect({
          source:"usecase_6",
          target:"usecase_7"
      }, stateMachineConnector);
    })
  </script>
